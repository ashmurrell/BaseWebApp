<html lang="en">
<head>
    <% include ../helpers/head %>
</head>

<body>

  <!-- the classes 'page-header' and 'btn btn-default' create bootstrap components.  -->
  <!-- Check out more components here: http://getbootstrap.com/components/ -->
  <div class="page-header">
    <% include ../helpers/navbar %>
    <h1>Contact Submission Form</h1>
  </div>

 
  <form class= onSubmit= "submitContact()" action="#" name="contactform">
    <div class="form-group">
    <label for= "first-name">First Name </label>
    <br><input type="text" id="first-name" required><br>
    <label for= "last-name"> Last Name </label>
    <br><input type="text" id="last-name" required><br>
    <label for= "email"> Email</label>
    <br><input type="email" id="email" required><br>
    </div>
    <div class ="form-group form-check">
    <input type="checkbox" class="form-check-input" id="data-consent" required>
    <label class="form-check-label" for="data-consent">I agree to share my data with Able marketing</label>
    <br><button type "submit" class= "btn btn-primary">Submit</button>
</form>


